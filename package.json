{
  "name": "inventory-ad",
  "version": "0.1.0",
  "description": "Inventory management system for the AD",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:reset": "npx prisma migrate reset && npx prisma generate && npx prisma db push && npx prisma db seed",
    "generate:mock": "npx tsx src/prisma/mock.ts",
    "generate:diagram" : "npx tsuml2 --tsconfig \"./tsconfig.json\" --glob \"./src/types/model.ts\" --outMermaidDsl \"./assets/class-diagram.dsl\""
  },
  "prisma": {
    "seed": "npx tsx src/prisma/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "dotenv": "^16.4.5",
    "lucide-react": "^0.365.0",
    "next": "14.2.1",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.3",
    "sonner": "^1.4.41",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "colors": "^1.4.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
